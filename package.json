  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --port 4200",
    "start2": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --port 4200 --no-live-reload --host 0.0.0.0",
    "start-dist": "cd dist-start-local/app-shell && angular-http-server -p 4200",
    "start-uat": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --port 4200 -c uat",
    "start-uat2": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --port 4200 -c uat --no-live-reload --host 0.0.0.0",
    "build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --build-optimizer --common-chunk --vendor-chunk --progress",
    "build-dev": "node --max_old_space_size=7168 ./node_modules/@angular/cli/bin/ng build --aot -c dev --build-optimizer --common-chunk --vendor-chunk --progress",
    "build-sit": "node --max_old_space_size=7168 ./node_modules/@angular/cli/bin/ng build --aot -c sit --build-optimizer --common-chunk --vendor-chunk --progress",
    "build-uat": "node --max_old_space_size=7168 ./node_modules/@angular/cli/bin/ng build --aot -c uat --build-optimizer --common-chunk --vendor-chunk --progress",
    "build-prod": "node --max_old_space_size=7168 ./node_modules/@angular/cli/bin/ng build --aot -c production --build-optimizer --common-chunk --vendor-chunk --progress",
    "build-lib": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build msb-bpm-core && cd dist/msb-bpm-core && npm pack && npx copyfiles -f *.tgz out ../../lib-versions",
    "update-lib": "npm run build-lib && npm i && npm run start",
    "update-lib2": "npm run build-lib && npm i && npm run start2",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
